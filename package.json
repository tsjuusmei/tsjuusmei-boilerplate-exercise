{
  "name": "chicle",
  "version": "0.1.0",
  "description": "Chicle, the development boilerplate of Yummygum.",
  "main": "index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "preexport": "yarn build",
    "commit": "git cz",
    "release": "standard-version",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -c .storybook -s ./public -o build",
    "lint": "eslint src/**/*.tsx  && stylelint src/**/*.scss",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "new-component": "node scripts/generate-component.js",
    "new-ts-component": "node scripts/generate-ts-component.js",
    "test": "jest --no-watchman"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [],
  "author": "Yummygum",
  "license": "CC BY-NC-ND",
  "private": true,
  "homepage": "https://github.com/Yummygum/next-boilerplate#readme",
  "dependencies": {
    "framer-motion": "2.9.4",
    "glob": "7.1.6",
    "next": "10.0.1",
    "next-seo": "4.17.0",
    "postcss-preset-env": "6.7.0",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "1.29.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/eslint-parser": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.5",
    "@babel/preset-typescript": "7.12.1",
    "@storybook/addon-a11y": "6.0.28",
    "@storybook/addon-backgrounds": "6.0.28",
    "@storybook/addon-viewport": "6.0.28",
    "@storybook/addons": "6.0.28",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.0.28",
    "@testing-library/react": "11.1.2",
    "@testing-library/react-hooks": "3.4.2",
    "@types/jest": "^26.0.15",
    "@types/node": "14.14.7",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@typescript-eslint/eslint-plugin": "4.8.0",
    "@typescript-eslint/parser": "4.8.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.1",
    "css-loader": "5.0.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.13.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "git-cz": "4.7.3",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "jest-css-modules": "2.1.0",
    "react-docgen-typescript-loader": "3.7.2",
    "sass-loader": "10.1.0",
    "sass-resources-loader": "2.1.1",
    "standard-version": "9.0.0",
    "storybook": "6.0.28",
    "style-loader": "2.0.0",
    "stylelint": "13.7.2",
    "stylelint-8-point-grid": "1.0.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-standard": "20.0.0",
    "typescript": "4.0.5",
    "typescript-plugin-css-modules": "2.8.0"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
